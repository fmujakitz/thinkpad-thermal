{
  "name": "thinkpad-thermal",
  "version": "1.0.0",
  "author": "Faris Mujakic <faris.mujakic@gmail.com>",
  "description": "Display ThinkPad thermal and fan status using /proc/acpi/ibm/thermal and /proc/acpi/ibm/fan.",
  "private": true,
  "license": "MIT",
  "url": "https://github.com/fmujakitz/thinkpad-thermal",
  "scripts": {
    "types:clean": "rm -rf ./@types",
    "types:lock": "rm -rf docs-lock.json && cross-env XDG_DATA_DIRS=$XDG_DATA_DIRS:/usr/share/gir-1.0:/usr/share/gnome-shell:/usr/lib/mutter-10:/usr/lib64/mutter-10 yarn run gi-ts config --lock",
    "types:build": "yarn run types:clean && gi-ts generate",
    "clean": "yarn run clean:ts && yarn run types:build",
    "clean:ts": "rm -rf ./dist",
    "build": "yarn run types:lock && yarn run types:build && yarn run build:ts",
    "build:ts": "yarn run clean:ts && rollup -c",
    "build:package": "yarn run build:ts && cd ./dist && zip -qr 'thinkpadthermal@moonlight.drive.vk.gmail.com.zip' .",
    "build:dev": "yarn build:ts && cp dist/* ~/.local/share/gnome-shell/extensions/thinkpadthermal@moonlight.drive.vk.gmail.com -r",
    "watch": "yarn run build && yarn run rollup -c --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@gi.ts/cli": "^1.5.10",
    "@gi.ts/lib": "^1.5.13",
    "@gi.ts/parser": "^1.5.10",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.4",
    "@types/events": "^3.0.0",
    "@types/gettext-parser": "^4.0.1",
    "@types/highlight.js": "^10.1.0",
    "@types/is-url": "^1.2.30",
    "@types/prismjs": "^1.26.0",
    "cross-env": "^7.0.3",
    "fill-pot-po": "^2.1.9",
    "gettext-extractor": "^3.5.4",
    "gettext-parser": "^6.0.0",
    "glob": "^8.0.3",
    "rollup": "^2.78.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-visualizer": "^5.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@gi-types/adw1": "^1.1.1",
    "@gi-types/atk2": "^2.38.1",
    "@gi-types/clutter10": "^10.0.1",
    "@gi-types/gdk4": "^4.0.1",
    "@gi-types/gdkpixbuf2": "^2.0.2",
    "@gi-types/gio2": "^2.72.1",
    "@gi-types/glib2": "^2.72.1",
    "@gi-types/gobject2": "^2.72.1",
    "@gi-types/graphene1": "^1.0.1",
    "@gi-types/gtk4": "^4.6.1",
    "@gi-types/meta10": "^10.0.1",
    "@gi-types/pango1": "^1.50.1",
    "@gi-types/shell0": "^0.1.1",
    "@gi-types/soup3": "^3.0.1",
    "@gi-types/st1": "^1.0.1"
  }
}